<div class="row center">

    <h1>Actions</h1>
    {{!-- <div class="center nameTable"> --}}
    <table class="center" name="users">
        <tr>
            <th>Names</th>
            <th>Count</th>
        </tr>
        {{#each actions}}
        <tr>
            <td><a href="/counter/{{greet_name}}">{{greet_name}}</a></td>
            <td>{{greet_count}}</td>

        </tr>
        {{/each}}
    </table>
    <div class="col-12">
        <form action="/backHome" method="POST">
            <button class="button-primary greetButton" type="submit" name="button">Home</button>
        </form>
    </div>

</div>